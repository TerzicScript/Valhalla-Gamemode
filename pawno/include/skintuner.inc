stock SetPlayerAttachment(playerid, slot, modelid)
{
	if(slot > MAX_PLAYER_ATTACHED_OBJECTS || slot < 0) return false;

	if(IsPlayerAttachedObjectSlotUsed(playerid, slot)) RemovePlayerAttachedObject(playerid, slot);

    static skin = GetPlayerSkin(playerid);
	switch(modelid)
	{
	    case 19007..19036,19138..19140://normal ped glasses & police glasses
	    {
	        if(Attachment[playerid][slot][a_type] == GLASSES) RemovePlayerAttachedObject(playerid, Attachment[playerid][slot][a_slot]);
	        Attachment[playerid][slot][a_type] = GLASSES;
	        return SetPlayerAttachedObject(playerid, slot, modelid, 2, gGlassesOffset[skin][0], gGlassesOffset[skin][1], gGlassesOffset[skin][2], gGlassesOffset[skin][3], gGlassesOffset[skin][4], gGlassesOffset[skin][5], gGlassesOffset[skin][6], gGlassesOffset[skin][6], gGlassesOffset[skin][6]);
	    }
	    case 19516,18975,19077,19517://hair type 1, white and brown
	    {
	    	if(Attachment[playerid][slot][a_type] == HAIRS) RemovePlayerAttachedObject(playerid, Attachment[playerid][slot][a_slot]);
	        Attachment[playerid][slot][a_type] = HAIRS;
	        return SetPlayerAttachedObject(playerid, slot, modelid, 2, gSkinsOffset[skin][0], gSkinsOffset[skin][1], gSkinsOffset[skin][2], gSkinsOffset[skin][3], gSkinsOffset[skin][4], gSkinsOffset[skin][5], gSkinsOffset[skin][8], gSkinsOffset[skin][8], gSkinsOffset[skin][8]);
	    }
	    case 18926..18935://caps
	    {
	        //invalid skins
	        switch(skin)
			{
				case 1,2,3,4,5,7,12,15,17,18,21,23,26,27,30,32,33,34,40,41,50,51,60,64,73,76,85,98,103,106,114,118,136,142,148,152,154,157,160,166,172,197,204,207,214,241,245,248,252,254,259,268,269,272,276,277,278,282,283,284,286,287,288,292:
				{
					return false;
				}
	        }
			if(Attachment[playerid][slot][a_type] == CAPS) RemovePlayerAttachedObject(playerid, Attachment[playerid][slot][a_slot]);
	        Attachment[playerid][slot][a_type] = CAPS;
			SetPlayerAttachedObject(playerid, slot, modelid, 2, gCapsOffset[skin][0], gCapsOffset[skin][1], gCapsOffset[skin][2], gCapsOffset[skin][3], gCapsOffset[skin][4], gCapsOffset[skin][5]);
	    }
	}
 	Attachment[playerid][slot][a_modelid] = modelid;
	Attachment[playerid][slot][a_slot] = slot;
	return true;
}

stock RemovePlayerAttachment(playerid, slot)
{
	if(slot > MAX_PLAYER_ATTACHED_OBJECTS || slot < 0) return false;

 	Attachment[playerid][slot][a_type] = -1;
	Attachment[playerid][slot][a_modelid] = -1;
	Attachment[playerid][slot][a_slot] = -1;
	return RemovePlayerAttachedObject(playerid, slot);
}
